{
	"name": "utd-degree-planner-and-auditor",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"client": "vite",
		"server": "nodemon --watch src/server -e js,ts --env-file .env --exec tsx src/server/index.ts",
		"dev": "concurrently \"npm run server\" \"npm run client\"",
		"build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json|tsx)\""
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	},
	"dependencies": {
		"@headlessui/react": "^2.2.0",
		"@heroicons/react": "^2.2.0",
		"@prisma/client": "^6.5.0",
		"@tailwindcss/vite": "^4.0.8",
		"axios": "^1.8.4",
		"body-parser": "^2.2.0",
		"cookie-parser": "^1.4.7",
		"cors": "^2.8.5",
		"cytoscape": "^3.31.2",
		"cytoscape-dagre": "^2.5.0",
		"express": "^5.1.0",
		"http-status-codes": "^2.3.0",
		"immer": "^10.1.1",
		"jsonwebtoken": "^9.0.2",
		"react": "^19.0.0",
		"react-dom": "^19.0.0",
		"react-router": "^7.5.0",
		"tailwindcss": "^4.0.8",
		"zod": "^3.24.2"
	},
	"devDependencies": {
		"@eslint/js": "^9.19.0",
		"@types/cookie-parser": "^1.4.8",
		"@types/cors": "^2.8.17",
		"@types/cytoscape-dagre": "^2.3.3",
		"@types/express": "^5.0.1",
		"@types/node": "^22.13.14",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react": "^4.3.4",
		"concurrently": "^9.1.2",
		"eslint": "^9.19.0",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.14.0",
		"nodemon": "^3.1.9",
		"prettier": "^3.5.2",
		"prisma": "^6.5.0",
		"tsx": "^4.19.3",
		"typescript": "~5.7.2",
		"typescript-eslint": "^8.22.0",
		"vite": "^6.2.3"
	}
}
